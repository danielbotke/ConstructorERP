	<queryString>
		<![CDATA[SELECT
     renegociacao.`descricao` AS renegociacao_descricao,
     DATE_FORMAT(historico.`dataHistorico`,'%d/%m/%Y') AS historico_dataHistorico,
     historico.`descricao` AS historico_descricao
FROM
     `renegociacao` renegociacao, 
     `historico` historico 
WHERE
     renegociacao.`encerrada` = 0 and
     historico.`idObjetoHistorico` = renegociacao.`id`
ORDER BY
     historico.idObjetoHistorico, historico.dataHistorico]]>
